<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cartSQL">
	<select id="get_cart" parameterType="Integer" resultType="cart">
		select * from TBL_CART where productCode = #{productCode}
	</select>
</mapper>